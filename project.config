[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/Xavier Caron (xavcaron)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Xavier Caron (xavcaron)
	label-Verified = -1..+1 group user/Xavier Caron (xavcaron)
	push = group user/Xavier Caron (xavcaron)
	read = group user/Xavier Caron (xavcaron)
	rebase = group user/Xavier Caron (xavcaron)
	removeReviewer = group user/Xavier Caron (xavcaron)
	submit = group user/Xavier Caron (xavcaron)
[access "refs/heads/*"]
	create = group user/Xavier Caron (xavcaron)
	push = +force group user/Xavier Caron (xavcaron)
	pushMerge = group user/Xavier Caron (xavcaron)
	read = group user/Xavier Caron (xavcaron)
[access "refs/tags/*"]
	push = group user/Xavier Caron (xavcaron)
	push = +force group user/Xavier Caron (xavcaron)
